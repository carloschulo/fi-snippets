{
  "Layout maxwidth": {
    "prefix": "laymaxwid",
    "body": [
      " - type: layout",
      "   metadata: grid/maxwidth",
      "   path: grid/maxwidth",
      "   regions:",
      "     - id: region1",
      "       blocks:",
      "         ${0:some other blocks here}"
    ],
    "description": "Layout maxwidth"
  },
  "Layout Aligment": {
    "prefix": "layal",
    "body": [
      "- type: layout",
      "  path: grid/alignment",
      "  metadata: alignment",
      "  horizontal: ${1:left,center, or right}",
      "  vertical: ${2:top, middle, or bottom}",
      "  regions:",
      "    - id: region1",
      "      blocks:",
      "        ${0:some other blocks here}"
    ],
    "description": "Layout Aligment"
  },
  "Layout centered": {
    "prefix": "laycen",
    "body": [
      "- type: layout",
      "  path: grid/centered",
      "  metadata: centered",
      "  regions:",
      "    - id: region1",
      "      blocks:",
      "        ${0:some other blocks here}"
    ],
    "description": "Layout centered"
  },
  "Layout fifty-fifty": {
    "prefix": "layfif",
    "body": [
      "- type: layout",
      "  metadata: fifty-fifty",
      "  path: grid/fifty-fifty",
      "  reverse_stack: ${1:true or false}",
      "  regions:",
      "    - id: region1",
      "      blocks:",
      "        ${2:some other blocks here}",
      "    - id: region2",
      "      blocks:",
      "        ${0:some other blocks here}"
    ],
    "description": "Layout fifty-fifty"
  },
  "Layout fit-remainder": {
    "prefix": "layfitrem",
    "body": [
      "- type: layout",
      "  metadata: grid/fit-remainder",
      "  path: grid/fit-remainder",
      "  regions:",
      "    - id: region1",
      "      disableMargin: ${1:true or false}",
      "      disablePadding: ${2:true or false}",
      "      blocks:",
      "        ${3:more blocks}",
      "    - id: region2",
      "      disableMargin: ${1:true or false}",
      "      disablePadding: ${2:true or false}",
      "      blocks:",
      "        ${0:more blocks}"
    ],
    "description": "Layout fit-remainder"
  },
  "Layout forty-sixty": {
    "prefix": "layforsix",
    "body": [
      "- type: layout",
      "  metadata: forty-sixty",
      "  path: grid/forty-sixty",
      "  regions:",
      "    - id: region1",
      "      disableMargin: ${1:true or false}",
      "      disablePadding: ${2:true or false}",
      "      blocks:",
      "        ${3:more blocks}",
      "    - id: region2",
      "      disableMargin: ${1:true or false}",
      "      disablePadding: ${2:true or false}",
      "      blocks:",
      "        ${0:more blocks}"
    ],
    "description": "Layout forty-sixty"
  },
  "Layout four-column": {
    "prefix": "layfourcol",
    "body": [
      "- type: layout",
      "  metadata: four-column",
      "  path: grid/four-column",
      "  regions:",
      "    - id: region1",
      "      ${1:region params?}",
      "      blocks:",
      "        ${2:more blocks}",
      "    - id: region2",
      "      ${1:region params?}",
      "      blocks:",
      "        ${3:more blocks}",
      "    - id: region3",
      "      ${1:region params?}",
      "      blocks:",
      "        ${4:more blocks}",
      "    - id: region4",
      "      ${1:region params?}",
      "      blocks:",
      "        ${0:more blocks}"
    ],
    "description": "Layout four-column"
  },
  "Layout hatLabel": {
    "prefix": "layhatlab",
    "body": [
      "- type: layout",
      "  metadata: hatlabel",
      "  path: grid/hatlabel",
      "  class0: body",
      "  class1: label",
      "  regions:",
      "    - id: region1",
      "      ${1:region params?}",
      "      blocks:",
      "        ${2:more blocks}",
      "    - id: region2",
      "      ${1:region params?}",
      "      blocks:",
      "        ${0:more blocks}",
      ""
    ],
    "description": "Layout hatLabel"
  },
  "Layout row": {
    "prefix": "layrow",
    "body": [
      "- type: layout",
      "  metadata: row",
      "  path: grid/row",
      "  regions:",
      "    - id: region1",
      "      ${1:region params?}",
      "      blocks:",
      "        ${0:more blocks}"
    ],
    "description": "Layout row"
  },
  "Layout three-column": {
    "prefix": "laythreecol",
    "body": [
      "- type: layout",
      "  metadata: row",
      "  path: grid/row",
      "  regions:",
      "    - id: region1",
      "      ${1:region params?}",
      "      blocks:",
      "        ${2:more blocks}",
      "    - id: region2",
      "      ${1:region params?}",
      "      blocks:",
      "        ${3:more blocks}",
      "    - id: region3",
      "      ${1:region params?}",
      "      blocks:",
      "        ${0:more blocks}"
    ],
    "description": "Layout three-column"
  },
  "Component card flip": {
    "prefix": "compcardflip",
    "body": [
      "- type: component",
      "  configs: true",
      "  path: partial/molecules/card",
      "  parameters:",
      "    size: ${1:small, medium, large, full]} ",
      "    cards:",
      "      - blocks:",
      "          - type: layout",
      "            metadata: grid/row",
      "            path: grid/row",
      "            regions:",
      "              - id: region1",
      "                background:",
      "                  type: image",
      "                  settings:",
      "                    path: ${2:images/modules/M02/someimage.png}",
      "                minHeight:",
      "                  override: true",
      "                  value: ${3:160px}",
      "        animation: 'if: click, do: flipInY animated, after: removeAnim'",
      "        back:",
      "          blocks:",
      "            - type: component",
      "              path: partial/organelles/background",
      "              configs: true",
      "              parameters:",
      "                type: color",
      "                settings:",
      "                  color: ${4:'#FA9E89'}",
      "                blocks:",
      "                  - type: layout",
      "                    metadata: grid/row",
      "                    path: grid/row",
      "                    regions:",
      "                      - id: region1",
      "                        blocks:",
      "                          - type: header",
      "                            class: h4",
      "                            h2:",
      "                              - flipcardHeader",
      "            - type: layout",
      "              metadata: grid/row",
      "              path: grid/row",
      "              regions:",
      "                - id: region1",
      "                  blocks:",
      "                    - type: text",
      "                      class: p2",
      "                      items:",
      "                        - description: flipcardBackText"
    ],
    "description": "Component card flip"
  },
  "Component card": {
    "prefix": "compcard",
    "body": [
      "- type: component",
      "  configs: true",
      "  path: partial/molecules/card",
      "  parameters:",
      "    size: ${1:small, medium, large, full]} ",
      "    cards:",
      "      - blocks:",
      "          - type: layout",
      "            metadata: grid/row",
      "            path: grid/row",
      "            regions:",
      "              - id: region1",
      "                background:",
      "                  type: image",
      "                  settings:",
      "                    path: ${2:images/modules/M02/someimage.png}",
      "                minHeight:",
      "                  override: true",
      "                  value:${3:160px}",
      "${0:more blocks}"
    ],
    "description": "Component card"
  }
}